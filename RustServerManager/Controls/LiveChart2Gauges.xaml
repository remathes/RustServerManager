<UserControl x:Class="RustServerManager.Controls.LiveChart2Gauges"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:lvc="clr-namespace:LiveChartsCore.SkiaSharpView.WPF;assembly=LiveChartsCore.SkiaSharpView.WPF"
             xmlns:vms="clr-namespace:RustServerManager.Controls"
             MaxWidth="300"
             MaxHeight="300">
    <lvc:PieChart x:Name="Gauges"
        InitialRotation="-225"
        MaxAngle="270"
        MinValue="0"
        MaxValue="100">
    </lvc:PieChart>
</UserControl>